/**
 * Nand gate: out = a Nand b.
 */

CHIP Nand {

    IN  a, b;
    OUT out;
	Nand(a = 1, b = 1, out = 1);
	Nand(a = 0, b = 0, out = 0);
	Nand(a = 1, b = 0, out = 0);
	Nand(a = 0, b = 1, out = 0);
}
